{
  "schemaVersion": 1,
  "id": "skylands-custom",
  "version": "${version}",
  "name": "Skylands (custom)",
  "description": "Simple Skyblock Islands generator and manager.\nModified version of Skylands",
  "authors": [
    "PinkGoosik",
    "Awakened Redstone"
  ],
  "contributors": [
    "KujouMolean"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/skylands",
    "sources": "https://github.com/tyap-lyap/skylands",
    "issues": "https://github.com/tyap-lyap/skylands/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/skylands/icon.png",
  "accessWidener": "skylands.accesswidener",
  "environment": "*",
  "entrypoints": {
    "main": [
      "skylands.SkylandsMain"
    ],
    "cardinal-components": [
      "skylands.data.SkylandComponents"
    ]
  },
  "mixins": [
    "skylands.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "skylands:world_data",
      "skylands:player_data"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "fabric-api": ">=0.76.0",
    "minecraft": "1.19.4",
    "java": ">=17"
  },
  "breaks": {
    "skylands": "*"
  }
}
